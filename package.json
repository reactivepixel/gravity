{
  "name": "gravity",
  "version": "1.0.0",
  "description": "",
  "main": ".eslintrc.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:react": "concurrently --raw \"npm run api\" \"npm run react\"",
    "react": "cd reactjs && npm run start",
    "api": "cd api; nodemon server.js",
    "start": "npm run dev:react",
    "dirtyInstall": "npm i; cd api; npm i; cd ../reactjs; npm i; touch .env; cd ..;"
  },
  "keywords": [],
  "author": "Chapman",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "concurrently": "^4.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-app": "^4.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^13.1.0",
    "nodemon": "^1.18.11"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
